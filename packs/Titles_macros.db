{ "_id": "UusDmShWkTEot2lx", "name": "Heist Hero - Mother Hen", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=UusDmShWkTEot2lx\n//@name=Heist Hero - Mother Hen\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Heist Hero - Mother Hen\",\n  description: `You can spend 10 uninterrupted minutes to psionically enhance up to five willing creatures within 10 squares of you who understand a language you know. For the next hour, you and each target can communicate telepathically with each other no matter the distance between you.`,\n  source: \"Title\"\n});" }
{ "_id": "D5uawIKCSn781LqE", "name": "Heist Hero - Sneakers", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=D5uawIKCSn781LqE\n//@name=Heist Hero - Sneakers\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Heist Hero - Sneakers\",\n  description: `You gain the Sneak skill. If you already have this skill, you instead gain an edge on tests made using the Sneak skill. During group tests, you can both use the Sneak skill and assist another hero using the Sneak skill.`,\n  source: \"Title\"\n});" }
{ "_id": "tn5Uf7HNzYDIbcvw", "name": "Heist Hero - Timely Distraction", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=tn5Uf7HNzYDIbcvw\n//@name=Heist Hero - Timely Distraction\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareAbility\").execute({\n  activeActor: actor,\n  name: \"Heist Hero - Timely Distraction\",\n  flavorText: \"Coming through with hot soup! I better watch out for that banana peel!\",\n  keywords: \"Ranged\",\n  type: \"Triggered\",\n  distance: \"Ranged 10\",\n  target: \"One creature\",\n  trigger: \"An ally makes a test to lie to, pick the pocket of, hide from, or sneak by the target and doesn’t like the outcome.\",\n  effect: \"You momentarily attract the target’s notice to let your ally reroll their test. Once you use this ability, you can’t use it again against the same target for 1 hour.\"\n});" }
{ "_id": "qAmipWkNdV4nlboO", "name": "Knight - Heraldic Fame", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=qAmipWkNdV4nlboO\n//@name=Knight - Heraldic Fame\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Knight - Heraldic Fame\",\n  description: `You earn 1 Renown.</p>\n    <p>If you take this title, you might occasionally be called upon to perform duties for the person who knighted you.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "nJELYmkIAW3to4Ne", "name": "Knight - Knightly Aegis", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=nJELYmkIAW3to4Ne\n//@name=Knight - Knightly Aegis\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Knight - Knightly Aegis\",\n  description: `Your Stamina maximum increases by 6.</p>\n    <p>If you take this title, you might occasionally be called upon to perform duties for the person who knighted you.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "EkQ13ywG9x8KDAVh", "name": "Knight - Knightly Challenge", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=EkQ13ywG9x8KDAVh\n//@name=Knight - Knightly Challenge\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareAbility\").execute({\n  activeActor: actor,\n  name: \"Knight - Knightly Challenge\",\n  flavorText: \"Have at thee!\",\n  resourceCost: 5,\n  keywords: \"Melee, Strike, Weapon\",\n  type: \"Main action\",\n  distance: \"Melee 1\",\n  target: \"One creature\",\n  powerRollStat: \"Might or Agility\",\n  tier1Effect: \"7 damage; taunted (save ends)\",\n  tier2Effect: \"11 damage; taunted (save ends)\",\n  tier3Effect: \"16 damage; taunted (save ends)\",\n  effect: \"You can end the taunted condition on the target as a free maneuver.\"\n});" }
{ "_id": "qs9eycpDZPl0QmOd", "name": "Master Librarian - Arcane Improvisation", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=qs9eycpDZPl0QmOd\n//@name=Master Librarian - Arcane Improvisation\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Master Librarian - Arcane Improvisation\",\n  description: `When you use a damage-dealing magic signature ability, you can change its damage type to acid, cold, corruption, fire, lightning, poison, or sonic damage.`,\n  source: \"Title\"\n});" }
{ "_id": "DQsFcn7O0MbPHZwv", "name": "Master Librarian - I Have Just the Book", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=DQsFcn7O0MbPHZwv\n//@name=Master Librarian - I Have Just the Book\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Master Librarian - I Have Just the Book\",\n  description: `If you start a Discover Lore project in your hero’s stronghold or other a permanent base of operations you immediately gain 60 project points toward the completion of that project. If the project costs 60 or fewer points, you complete it in 10 uninterrupted minutes without needing to use a respite activity.`,\n  source: \"Title\"\n});" }
{ "_id": "vgcNy7nFliz4SpUL", "name": "Master Librarian - Picked Up a Few Things", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=vgcNy7nFliz4SpUL\n//@name=Master Librarian - Picked Up a Few Things\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Master Librarian - Picked Up a Few Things\",\n  description: `You know a skill from the lore skill group.`,\n  source: \"Title\"\n});" }
{ "_id": "6YtRm01FKvQgW47I", "name": "Master Librarian - Polyglot", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=6YtRm01FKvQgW47I\n//@name=Master Librarian - Polyglot\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Master Librarian - Polyglot\",\n  description: `You know two languages. Additionally, the project goal for the Learn New Language project is halved for you.`,\n  source: \"Title\"\n});" }
{ "_id": "DPh8opLsCmBFtqXi", "name": "Special Agent - Boffin", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=DPh8opLsCmBFtqXi\n//@name=Special Agent - Boffin\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Special Agent - Boffin\",\n  description: `You gain a small magic spy device called a boffin. Once per encounter, you can activate one the following boffin properties as a maneuver:</p>\n    <ul>\n      <li>Make a test that uses the Disguise skill. You gain an edge on the test.</li>\n      <li>One mundane lock you touch is unlocked.</li>\n      <li>Choose a square within 10 squares, even if you don’t have line of effect to it. You can observe the area around that square as if you were in it.</li>\n      <li>You throw the boffin up to 10 squares, where it explodes in a 5 cube. Each creature in the area takes fire damage equal to 2d10 + your level. The boffin is permanently destroyed but can be replaced by your spymaster … though they don’t like doing so too often.</li>\n    </ul>`,\n  source: \"Title\"\n});" }
{ "_id": "STejDGsXZkU1cnyQ", "name": "Special Agent - Caustic Alchemy", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=STejDGsXZkU1cnyQ\n//@name=Special Agent - Caustic Alchemy\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Special Agent - Caustic Alchemy\",\n  description: `You have your choice of the 1st-level shadow college features Coat the Blade or Smoke Bomb. When you use that feature, you can’t spend insight unless you have the Insight class feature.`,\n  source: \"Title\"\n});" }
{ "_id": "k0hx6unK9lSUiA7m", "name": "Special Agent - Spy Ring", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=k0hx6unK9lSUiA7m\n//@name=Special Agent - Spy Ring\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Special Agent - Spy Ring\",\n  description: `You gain a piece of magic jewelry, such as a ring. As a main action while wearing the jewelry, you can take on the illusory appearance of an individual within 10 squares who you have line of effect to. This disguise lets you automatically succeed on tests made using the Disguise skill based solely on visual identification.`,\n  source: \"Title\"\n});" }
{ "_id": "nsO21fGigEkh3RCv", "name": "Sworn Hunter - Hunter’s Oath", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=nsO21fGigEkh3RCv\n//@name=Sworn Hunter - Hunter’s Oath\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Sworn Hunter - Hunter’s Oath\",\n  description: `As a main action, you swear a hunter’s oath against a creature within 10 squares who you have line of effect to. This oath lasts until the target dies or until you swear a hunter’s oath against a different creature. As long as the hunter’s oath lasts, you magically know the direction to the target if they are within 50 miles of you, and your damage-dealing abilities gain a +5 damage bonus against the target.`,\n  source: \"Title\"\n});" }
{ "_id": "7NkLTPjWeDZtsFyp", "name": "Sworn Hunter - Particular Set of Skills", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=7NkLTPjWeDZtsFyp\n//@name=Sworn Hunter - Particular Set of Skills\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Sworn Hunter - Particular Set of Skills\",\n  description: `You know a skill from the intrigue skill group.`,\n  source: \"Title\"\n});" }
{ "_id": "Z9kzBYOVLvG1UjSy", "name": "Sworn Hunter - We’re In This Together", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=Z9kzBYOVLvG1UjSy\n//@name=Sworn Hunter - We’re In This Together\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Sworn Hunter - We’re In This Together\",\n  description: `When you have a creature grabbed and take damage from an ability not used by that creature, the grabbed creature takes the same damage.`,\n  source: \"Title\"\n});" }
{ "_id": "mrdq8kGp6bTFM7l0", "name": "Undead Slain - Ghoul or Vampire", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=mrdq8kGp6bTFM7l0\n//@name=Undead Slain - Ghoul or Vampire\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Undead Slain - Ghoul or Vampire\",\n  description: `You return to life 1 minute after being killed with Stamina equal to your winded value. You gain corruption immunity equal to your level and when you make a melee free strike against an adjacent creature, you can bite that creature. If you do so and obtain a tier 3 outcome, you gain temporary Stamina equal to the damage dealt. If not lost beforehand, this temporary Stamina lasts until the end of your next respite.</p>\n    <p>If you die again, you rise as an undead creature under the Director’s control.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "UZTda5PJXs9u2BOV", "name": "Undead Slain - Incorporeal Undead", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=UZTda5PJXs9u2BOV\n//@name=Undead Slain - Incorporeal Undead\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Undead Slain - Incorporeal Undead\",\n  description: `You return to life 1 minute after being killed with Stamina equal to your winded value. You gain corruption immunity equal to your level and you can move through other creatures and objects. The first time in a combat round that you pass through a creature, that creature takes corruption damage equal to half your level. You don’t take damage from being force moved into objects.</p>\n    <p>If you die again, you rise as an undead creature under the Director’s control.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "qAtRuaPhgl3enco6", "name": "Undead Slain - Other Corporeal Undead", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=qAtRuaPhgl3enco6\n//@name=Undead Slain - Other Corporeal Undead\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Undead Slain - Other Corporeal Undead\",\n  description: `You return to life 1 minute after being killed with Stamina equal to your winded value. You gain corruption immunity equal to your level and when you are reduced to 0 Stamina by damage that isn’t fire or holy damage and your body isn’t destroyed, you can regain half your Stamina and fall prone. Once you use this benefit, you can’t use it again until you earn 10 or more Victories.</p>\n    <p>If you die again, you rise as an undead creature under the Director’s control.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "NuO0eAzq4LMEHiJ1", "name": "Unstoppable - From Hell’s Heart", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=NuO0eAzq4LMEHiJ1\n//@name=Unstoppable - From Hell’s Heart\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Unstoppable - From Hell’s Heart\",\n  description: `While you are winded, your melee strikes gain a +3 damage bonus.`,\n  source: \"Title\"\n});" }
{ "_id": "8qK1xr56nM9ZTLbe", "name": "Unstoppable - Furious Attack", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=8qK1xr56nM9ZTLbe\n//@name=Unstoppable - Furious Attack\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Unstoppable - Furious Attack\",\n  description: `Choose one signature ability from the fury class. You gain that ability regardless of whether your class and subclass allow you to take it. If this ability allows you to gain or spend ferocity, you can’t do so unless you have the Ferocity class feature.`,\n  source: \"Title\"\n});" }
{ "_id": "54hKY3JNIsgfAkCX", "name": "Unstoppable - Furious Charge", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=54hKY3JNIsgfAkCX\n//@name=Unstoppable - Furious Charge\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Unstoppable - Furious Charge\",\n  description: `When you use the Charge action, your strike made as part of that action gains a damage bonus equal to the number of squares you moved as part of the charge.`,\n  source: \"Title\"\n});" }
{ "_id": "Mas7FOqb65neYR4p", "name": "Armed and Dangerous", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=Mas7FOqb65neYR4p\n//@name=Armed and Dangerous\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Armed and Dangerous\",\n  description: `You can use and gain the benefits of kits.`,\n  source: \"Title\"\n});" }
{ "_id": "pGQJsmT2hlFw8fXd", "name": "Back From the Grave", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=pGQJsmT2hlFw8fXd\n//@name=Back From the Grave\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Back From the Grave\",\n  description: `You are restored to life. You gain the Tough But Withered signature trait from the revenant ancestry.`,\n  source: \"Title\"\n});" }
{ "_id": "iCEJDQ6df0yLgzY2", "name": "Demon Slayer - Demonic Lore", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=iCEJDQ6df0yLgzY2\n//@name=Demon Slayer - Demonic Lore\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Demon Slayer - Demonic Lore\",\n  description: `You know the Proto-Ctholl language. Additionally, when you deal damage using a magic ability, you can change the ability’s damage type to holy.</p>\n    <p>When you make a Presence test and roll a natural 5 or lower, you are cursed to communicate in only Proto-Ctholl for 1 minute, whether you know that language or not.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "vKEwpXDTc41yINAJ", "name": "Demon Slayer - Lethe", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=vKEwpXDTc41yINAJ\n//@name=Demon Slayer - Lethe\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Demon Slayer - Lethe\",\n  description: `While you are winded, your strikes gain a +5 damage bonus.</p>\n    <p>When you make a Presence test and roll a natural 5 or lower, you are cursed to communicate in only Proto-Ctholl for 1 minute, whether you know that language or not.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "tes4Mr7ogNx1JnHV", "name": "Demon Slayer - Made of Teeth", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=tes4Mr7ogNx1JnHV\n//@name=Demon Slayer - Made of Teeth\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Demon Slayer - Made of Teeth\",\n  description: `Your body can sprout teeth in unusual places. Whenever a creature makes physical contact with you or starts their turn touching you, you can deal 5 damage to them (no action required).</p>\n    <p>When you make a Presence test and roll a natural 5 or lower, you are cursed to communicate in only Proto-Ctholl for 1 minute, whether you know that language or not.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "y8dGwUhoV5I69cNO", "name": "Demon Slayer - Soulsight", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=y8dGwUhoV5I69cNO\n//@name=Demon Slayer - Soulsight\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Demon Slayer - Soulsight\",\n  description: `Any creature within 2 squares can’t be hidden from you.</p>\n    <p>When you make a Presence test and roll a natural 5 or lower, you are cursed to communicate in only Proto-Ctholl for 1 minute, whether you know that language or not.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "TIhle3ygJd8tn0YE", "name": "Diabolist - Devil Lore", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=TIhle3ygJd8tn0YE\n//@name=Diabolist - Devil Lore\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Diabolist - Devil Lore\",\n  description: `You know the Anjali language, and your understanding of this language helps you create irresistible supernatural effects. The potencies of your magic or psionic abilities that target Reason, Intuition, or Presence increase by 1.`,\n  source: \"Title\"\n});" }
{ "_id": "WgT8rcUGkLD1EqVv", "name": "Diabolist - Infernal Legacy", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=WgT8rcUGkLD1EqVv\n//@name=Diabolist - Infernal Legacy\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Diabolist - Infernal Legacy\",\n  description: `You gain 3 ancestry points to spend on purchased devil ancestry traits.`,\n  source: \"Title\"\n});" }
{ "_id": "8cai1Ju9pqd7kXhg", "name": "Diabolist - Sly Devil", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=8cai1Ju9pqd7kXhg\n//@name=Diabolist - Sly Devil\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Diabolist - Sly Devil\",\n  description: `You gain the Silver Tongue signature trait from the devil ancestry.`,\n  source: \"Title\"\n});" }
{ "_id": "wW8lBK9QvOuc1L6d", "name": "Diabolist - Untouched by Corruption", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=wW8lBK9QvOuc1L6d\n//@name=Diabolist - Untouched by Corruption\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Diabolist - Untouched by Corruption\",\n  description: `Whenever you use a damage-dealing ability, that ability can deal holy damage instead of its usual damage type.`,\n  source: \"Title\"\n});" }
{ "_id": "MS46qhpORwiktUln", "name": "Dragon Blooded - Dragon Scaled", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=MS46qhpORwiktUln\n//@name=Dragon Blooded - Dragon Scaled\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Dragon Blooded - Dragon Scaled\",\n  description: `Dragon scales grow on your body wherever the heart’s blood of the dragon touched you. You gain the Wyrmplate signature trait from the dragon knight ancestry.`,\n  source: \"Title\"\n});" }
{ "_id": "XbQND4EnaTcOIhAS", "name": "Dragon Blooded - Dragon Touched", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=XbQND4EnaTcOIhAS\n//@name=Dragon Blooded - Dragon Touched\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Dragon Blooded - Dragon Touched\",\n  description: `You gain 3 ancestry points to spend on purchased dragon knight ancestry traits.`,\n  source: \"Title\"\n});" }
{ "_id": "QrM6xpLd8WTKylgD", "name": "Fleet Admiral - First Mate", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=QrM6xpLd8WTKylgD\n//@name=Fleet Admiral - First Mate\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Fleet Admiral - First Mate\",\n  description: `You have a pirate retainer, such as a human warrior, chosen by the Director. This retainer’s level increases to your level.`,\n  source: \"Title\"\n});" }
{ "_id": "KxFiLNob501vIdc7", "name": "Fleet Admiral - Swashbuckler", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=KxFiLNob501vIdc7\n//@name=Fleet Admiral - Swashbuckler\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Fleet Admiral - Swashbuckler\",\n  description: `You can automatically climb at full speed while moving.`,\n  source: \"Title\"\n});" }
{ "_id": "5uQfE29BOyhmWI3c", "name": "Fleet Admiral - Treasure Keeper", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=5uQfE29BOyhmWI3c\n//@name=Fleet Admiral - Treasure Keeper\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Fleet Admiral - Treasure Keeper\",\n  description: `You earn 1 Wealth.`,\n  source: \"Title\"\n});" }
{ "_id": "hVquXfKGs2klS4F6", "name": "Fleet Admiral - Weather Wizard", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=hVquXfKGs2klS4F6\n//@name=Fleet Admiral - Weather Wizard\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Fleet Admiral - Weather Wizard\",\n  description: `Once per day, you can spend 10 uninterrupted minutes to magically alter mundane weather in a 5-mile radius around you. The weather moves with you and persists for 6 hours or until you dismiss it as a free maneuver. Choose from one of the following weather types:</p>\n    <p><em>Calm:</em> Wind-powered vessels and technology cease working.</p>\n    <p><em>Fog:</em> Visibility is reduced to 6 squares.</p>\n    <p><em>High Winds:</em> The speed of wind-powered vessels is doubled.</p>\n    <p><em>Light Winds:</em> No effects due to weather.</p>\n    <p><em>Storm:</em> The crew of an unsheltered wind-powered vessel must make a medium group Reason test. On a failure, the vessel needs repairs and moves at half speed until those repairs are made.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "jPWa2eQrkg0TwKR9", "name": "Maestro - Angelic Chorus", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=jPWa2eQrkg0TwKR9\n//@name=Maestro - Angelic Chorus\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Maestro - Angelic Chorus\",\n  description: `You can use the lessons of musical improvisation in combat. Choose one class act triggered action from the troubadour class. You gain that ability regardless of whether your class and subclass allow you to take it. If this ability allows you to gain or spend drama, you can gain or spend the Heroic Resource of your class in place of drama.`,\n  source: \"Title\"\n});" }
{ "_id": "ZRtbJTInMeAgPch8", "name": "Maestro - The Devil’s Chord", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=ZRtbJTInMeAgPch8\n//@name=Maestro - The Devil’s Chord\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareAbility\").execute({\n  activeActor: actor,\n  name: \"Maestro - The Devil’s Chord\",\n  flavorText: \"Helloooo Orden!\",\n  resourceCost: 9,\n  keywords: \"Area, Magic\",\n  type: \"Main action\",\n  distance: \"5 burst\",\n  target: \"Each creature in the area\",\n  powerRollStat: \"Presence\",\n  tier1Effect: \"You take 4 sonic damage unless you have the Performance skill.\",\n  tier2Effect: \"6 sonic damage; M < AVERAGE, weakened (save ends)\",\n  tier3Effect: \"10 sonic damage; M < STRONG, weakened and bleeding (save ends)\",\n  effect: \"The soul of any creature killed by this ability is dragged to Hell.\"\n});" }
{ "_id": "4EA5RplwDCbI7uoQ", "name": "Maestro - Music of the Spheres", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=4EA5RplwDCbI7uoQ\n//@name=Maestro - Music of the Spheres\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Maestro - Music of the Spheres\",\n  description: `As a main action, you sing or play a note as delicate and sharp as glass—and just as easily shattered. Until the start of your next turn, whenever a creature within 10 squares makes a strike, they take 8 sonic damage. Whenever you make a strike during that same period, you also take 8 sonic damage.`,\n  source: \"Title\"\n});" }
{ "_id": "FjbBPWz0J5yhlsLQ", "name": "Master Crafter - Masterpiece", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=FjbBPWz0J5yhlsLQ\n//@name=Master Crafter - Masterpiece\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Master Crafter - Masterpiece\",\n  description: `The armor, implement, or weapon can be imbued a fourth time, with any enhancement the item qualifies for.`,\n  source: \"Title\"\n});" }
{ "_id": "BhvZ80Ox7rC5QeEi", "name": "Master Crafter - Research Dividends", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=BhvZ80Ox7rC5QeEi\n//@name=Master Crafter - Research Dividends\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Master Crafter - Research Dividends\",\n  description: `You gain the item prerequisite for an armor, implement, or weapon enhancement of your choice. Additionally, you learn the project source language for that enhancement.`,\n  source: \"Title\"\n});" }
{ "_id": "3HVdXgEhQ8mS9yfb", "name": "Master Crafter - Skilled Hands", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=3HVdXgEhQ8mS9yfb\n//@name=Master Crafter - Skilled Hands\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Master Crafter - Skilled Hands\",\n  description: `You have a skill from the crafting skill group that would have been used during the creation of the prerequisite item. If you already have that skill, you instead gain an edge on tests made using the skill. Additionally, you gain a second skill of your choice from the crafting skill group.`,\n  source: \"Title\"\n});" }
{ "_id": "07G94TXfvx6FDetd", "name": "Master Crafter - Strong Hands Make Light Work", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=07G94TXfvx6FDetd\n//@name=Master Crafter - Strong Hands Make Light Work\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Master Crafter - Strong Hands Make Light Work\",\n  description: `Whenever you make a project roll, you can use Might as the project roll characteristic.`,\n  source: \"Title\"\n});" }
{ "_id": "XIt2C7fFlgrKDOZa", "name": "Noble - I Know How to Talk to These People", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=XIt2C7fFlgrKDOZa\n//@name=Noble - I Know How to Talk to These People\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Noble - I Know How to Talk to These People\",\n  description: `You gain an edge on Presence tests made to interact with royals, nobles, and their feudal followers, provided they are aware of your noble rank.</p>\n    <p>If you take this title, you might occasionally be called upon to perform duties for the person who granted your noble rank.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "2biDRTIJmN1Zolkv", "name": "Noble - Noble Splendor", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=2biDRTIJmN1Zolkv\n//@name=Noble - Noble Splendor\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Noble - Noble Splendor\",\n  description: `You earn 1 Renown and 1 Wealth.</p>\n    <p>If you take this title, you might occasionally be called upon to perform duties for the person who granted your noble rank.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "AUL4rNw0QM16EZK5", "name": "Noble - Retinue", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=AUL4rNw0QM16EZK5\n//@name=Noble - Retinue\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Noble - Retinue\",\n  description: `The number of followers you can recruit increases by two.</p>\n    <p>If you take this title, you might occasionally be called upon to perform duties for the person who granted your noble rank.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "bMz3G9xQlVWgF2pm", "name": "Planar Voyager - Prismacore Eyes", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=bMz3G9xQlVWgF2pm\n//@name=Planar Voyager - Prismacore Eyes\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Planar Voyager - Prismacore Eyes\",\n  description: `Exposure to prismacore has given your eyes a mirrorlike sheen. You have psychic immunity 10, creatures can’t use magic or psionic abilities or other effects to determine your location or read your thoughts unless you allow them to, and you gain a +3 bonus to stability against magic or psionic abilities.`,\n  source: \"Title\"\n});" }
{ "_id": "ZsaIm294HvkJB0ip", "name": "Planar Voyager - Stellar Knowledge", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=ZsaIm294HvkJB0ip\n//@name=Planar Voyager - Stellar Knowledge\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Planar Voyager - Stellar Knowledge\",\n  description: `You gain the Mechanics skill. If you already have this skill, you instead gain an edge on tests made using the skill. Additionally, you gain the item prerequisite and project source for a psionic trinket.`,\n  source: \"Title\"\n});" }
{ "_id": "csniWGDo5btmduPB", "name": "Planar Voyager - Time Raider Training", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=csniWGDo5btmduPB\n//@name=Planar Voyager - Time Raider Training\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Planar Voyager - Time Raider Training\",\n  description: `You gain 2 ancestry points to spend on purchased time raider ancestry traits.`,\n  source: \"Title\"\n});" }
{ "_id": "jPdGNhy0RKO6E4up", "name": "Scarred", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=jPdGNhy0RKO6E4up\n//@name=Scarred\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Scarred\",\n  description: `You gain a visible scar in a location of your choice. Additionally, your Stamina maximum increases by 20, and the creature who scarred you takes a bane on abilities against you.</p>\n    <p>You can gain this title multiple times. The second and each subsequent time that you gain it, your Stamima maximum doesn't increase.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "pCBwfnX9QyIqdlOu", "name": "Siege Breaker - Death From Above", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=pCBwfnX9QyIqdlOu\n//@name=Siege Breaker - Death From Above\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Siege Breaker - Death From Above\",\n  description: `When you gain an edge on an ability due to high ground, the ability gains a +8 damage bonus.`,\n  source: \"Title\"\n});" }
{ "_id": "zao8bJmFiqAwdZY2", "name": "Siege Breaker - Hold the Line", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=zao8bJmFiqAwdZY2\n//@name=Siege Breaker - Hold the Line\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Siege Breaker - Hold the Line\",\n  description: `While you’re within 5 squares of an ally, you and each ally within 5 squares of you gains a +3 bonus to stability.`,\n  source: \"Title\"\n});" }
{ "_id": "Tt4QH9J6hqBbczNP", "name": "Siege Breaker - Last Defender", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=Tt4QH9J6hqBbczNP\n//@name=Siege Breaker - Last Defender\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Siege Breaker - Last Defender\",\n  description: `Whenever an ally within 5 squares is reduced to 0 Stamina, you gain temporary Stamina equal to the ally’s level (or 1 if they have no level). If you already have temporary Stamina granted by this title, you increase your temporary Stamina by the amount you would have gained.`,\n  source: \"Title\"\n});" }
{ "_id": "flSnd17YzhHpRGF5", "name": "Teacher", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=flSnd17YzhHpRGF5\n//@name=Teacher\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Teacher\",\n  description: `You can travel with a student who shares your class. The student has the statistics of a 1st-level member of your class and has the same skills as you, but doesn’t engage in combat. They can perform any out-of-combat tasks a 1st-level member of your class can perform. Whenever they make a test to assist you in a task, they can’t obtain less than a tier 2 outcome on the test.`,\n  source: \"Title\"\n});" }
{ "_id": "VG95yjs42CptBNcY", "name": "Champion Competitor - Best of the Best", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=VG95yjs42CptBNcY\n//@name=Champion Competitor - Best of the Best\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Champion Competitor - Best of the Best\",\n  description: `A characteristic used during the competition increases by 1 (to a maximum of 6).</p>\n    <p>Additionally, choose a skill you used during the competition. You gain a +4 bonus to tests made using that skill instead of a +2 bonus.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "MDRFI3cJ2Gr8PEgy", "name": "Champion Competitor - Glory and Riches", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=MDRFI3cJ2Gr8PEgy\n//@name=Champion Competitor - Glory and Riches\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Champion Competitor - Glory and Riches\",\n  description: `A characteristic used during the competition increases by 1 (to a maximum of 6).</p>\n    <p>Additionally, you earn 2 Renown and 1 Wealth.</p>`,\n  source: \"Title\"\n});" }
