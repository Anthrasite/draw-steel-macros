{ "_id": "fV7zng8xQhP3stHE", "name": "Demigod - Missionaries", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=fV7zng8xQhP3stHE\n//@name=Demigod - Missionaries\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Demigod - Missionaries\",\n  description: `You earn 2 Renown.`,\n  source: \"Title\"\n});" }
{ "_id": "e27Uc0uv51iX94MW", "name": "Demigod - Worshippers", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=e27Uc0uv51iX94MW\n//@name=Demigod - Worshippers\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Demigod - Worshippers\",\n  description: `You magically hear prayers directed to you.`,\n  source: \"Title\"\n});" }
{ "_id": "1RlAI3byZJXmnO6U", "name": "Enlightened - Cosmic Revelation", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=1RlAI3byZJXmnO6U\n//@name=Enlightened - Cosmic Revelation\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Enlightened - Cosmic Revelation\",\n  description: `Your choice of your Reason or Intuition increases by 1 (to a maximum of 6).</p>\n    <p>Additionally, when you make a test with a skill from the lore skill group and obtain a tier 1 or tier 2 outcome, you can instead obtain a tier 3 outcome. Once you use this benefit, you can’t use it again until you earn 1 or more Victories.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "UFf8nNR1z6iQ9vbO", "name": "Enlightened - Mind Over Matter", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=UFf8nNR1z6iQ9vbO\n//@name=Enlightened - Mind Over Matter\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Enlightened - Mind Over Matter\",\n  description: `Your choice of your Reason or Intuition increases by 1 (to a maximum of 6).</p>\n    <p>Additionally, whenever you spend a Recovery, you can end one condition on yourself.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "XglE863arIw0Hv4M", "name": "Enlightened - Rearrange the Game Pieces", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=XglE863arIw0Hv4M\n//@name=Enlightened - Rearrange the Game Pieces\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Enlightened - Rearrange the Game Pieces\",\n  description: `Your choice of your Reason or Intuition increases by 1 (to a maximum of 6).</p>\n    <p>Additionally, you can reach behind the curtain and alter reality. At the start of combat, choose yourself or any creature within 5 squares. The chosen target must move up to their speed to a space you choose, but can’t enter damaging terrain or terrain that could impose a condition on them. The target doesn’t appear to move or teleport to that space—they are simply there. No one but you has any memory of the target’s previous position.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "sKGFZEndzhIaofOP", "name": "Forsaken - Brief Reunion", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=sKGFZEndzhIaofOP\n//@name=Forsaken - Brief Reunion\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Forsaken - Brief Reunion\",\n  description: `A characteristic of your choice increases by 1 (to a maximum of 6).</p>\n    <p>Additionally, while you’re winded, you can use a maneuver to summon the artifact to your hand. It disappears at the end of your next turn. Once you use this benefit, you can’t use it again until you earn 1 or more Victories.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "EzLgRMPvxwUcKYas", "name": "Forsaken - Perfect Protection", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=EzLgRMPvxwUcKYas\n//@name=Forsaken - Perfect Protection\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Forsaken - Perfect Protection\",\n  description: `A characteristic of your choice increases by 1 (to a maximum of 6).</p>\n    <p>Additionally, the Director chooses a damage type that is dealt by or thematically related to the artifact—for instance, holy for the Blade of a Thousand Years, psychic for the Encepter, or corruption for the Mortal Coil. You have immunity all to the chosen damage type.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "SBpZDrJYG8EKTeHb", "name": "Forsaken - Poor Compensation", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=SBpZDrJYG8EKTeHb\n//@name=Forsaken - Poor Compensation\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Forsaken - Poor Compensation\",\n  description: `A characteristic of your choice increases by 1 (to a maximum of 6).</p>\n    <p>Additionally, instead of disappearing or otherwise departing, the artifact turns into a trinket or leveled treasure of the Director’s choice that has the same approximate shape as the lost item—for instance, any magic sword for the Blade of a Thousand Years, any implement for the Encepter, or a Thief of Joy or any other torque for the Mortal Coil.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "XZODqK6tm8I4Lcu3", "name": "Monarch - Crown Jewels", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=XZODqK6tm8I4Lcu3\n//@name=Monarch - Crown Jewels\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Monarch - Crown Jewels\",\n  description: `You gain one of your nation’s treasures—a trinket of the Director’s choice.</p>\n    <p>If you take this title, you might be called upon to perform duties for your nation.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "uWUJj9h5PaD7eQbs", "name": "Monarch - Royal Authority", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=uWUJj9h5PaD7eQbs\n//@name=Monarch - Royal Authority\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Monarch - Royal Authority\",\n  description: `Inhabitants of your nation must obey your lawful orders or suffer the consequences.</p>\n    <p>If you take this title, you might be called upon to perform duties for your nation.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "bhC0xo9Fk16jgYKz", "name": "Monarch - Royal Fame", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=bhC0xo9Fk16jgYKz\n//@name=Monarch - Royal Fame\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Monarch - Royal Fame\",\n  description: `You earn 2 Renown.</p>\n    <p>If you take this title, you might be called upon to perform duties for your nation.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "LdDF4lqHUR2mWENG", "name": "Monarch - Royal Majesty", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=LdDF4lqHUR2mWENG\n//@name=Monarch - Royal Majesty\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Monarch - Royal Majesty\",\n  description: `Your choice of your Might or Presence increases by 1 (to a maximum of 6).</p>\n    <p>If you take this title, you might be called upon to perform duties for your nation.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "SHIc7Eyf8obagZvn", "name": "Monarch - Royal Retinue", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=SHIc7Eyf8obagZvn\n//@name=Monarch - Royal Retinue\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Monarch - Royal Retinue\",\n  description: `The number of followers you can recruit increases by 2.</p>\n    <p>If you take this title, you might be called upon to perform duties for your nation.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "Vj2MJDm1A6EoRIvP", "name": "Monarch - Royal Wealth", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=Vj2MJDm1A6EoRIvP\n//@name=Monarch - Royal Wealth\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Monarch - Royal Wealth\",\n  description: `You earn 2 Wealth.</p>\n    <p>If you take this title, you might be called upon to perform duties for your nation.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "tj2wR8fUG3hISMcz", "name": "Peace Bringer - Calm Heads Prevail", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=tj2wR8fUG3hISMcz\n//@name=Peace Bringer - Calm Heads Prevail\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Peace Bringer - Calm Heads Prevail\",\n  description: `Your choice of your Reason or Presence increases by 1 (to a maximum of 6).</p>\n    <p>Additionally, when you make a test to stop combat and start a negotiation, you always obtain a tier 3 outcome.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "i7d9HyE6P5rWeDaR", "name": "Peace Bringer - Drop Your Sword", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=i7d9HyE6P5rWeDaR\n//@name=Peace Bringer - Drop Your Sword\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Peace Bringer - Drop Your Sword\",\n  description: `Your choice of your Reason or Presence increases by 1 (to a maximum of 6).</p>\n    <p>Additionally, when you succeed on a test using the Intimidate skill, you can cause affected creatures to drop any items they are holding.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "Ds75JRdN1GnKp2ik", "name": "Peace Bringer - Hear Me Out", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=Ds75JRdN1GnKp2ik\n//@name=Peace Bringer - Hear Me Out\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Peace Bringer - Hear Me Out\",\n  description: `Your choice of your Reason or Presence increases by 1 (to a maximum of 6).</p>\n    <p>Additionally, while you are present in a negotiation, an NPC’s starting patience increases by 3 (to a maximum of 5).</p>`,\n  source: \"Title\"\n});" }
{ "_id": "1YQdKv3F6VIM9uhE", "name": "Peace Bringer - Many Paths to Peace", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=1YQdKv3F6VIM9uhE\n//@name=Peace Bringer - Many Paths to Peace\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Peace Bringer - Many Paths to Peace\",\n  description: `Your choice of your Reason or Presence increases by 1 (to a maximum of 6).</p>\n    <p>Additionally, when you make a test with a skill from the interpersonal skill group, you can use any characteristic of your choice for the test.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "vf8zbkAj2yiJoxeV", "name": "Reborn - Holy Weapon", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=vf8zbkAj2yiJoxeV\n//@name=Reborn - Holy Weapon\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Reborn - Holy Weapon\",\n  description: `A god or other powerful being has determined that it is not yet your time to die. Your body fades away, but you reappear alive 24 hours later in a location of your choice. You regain all your Stamina and Recoveries, and your choice of your Intuition or Presence increases by 1 (to a maximum of 6).</p>\n    <p>Additionally, you have a leveled treasure of the Director’s choice.</p>\n    <p>If a hakaan gains this title as a result of using their Doomsight trait, they lose that trait.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "iRpP3Wv25moC98f6", "name": "Reborn - Kill Me Once, Shame On You", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=iRpP3Wv25moC98f6\n//@name=Reborn - Kill Me Once, Shame On You\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Reborn - Kill Me Once, Shame On You\",\n  description: `A god or other powerful being has determined that it is not yet your time to die. Your body fades away, but you reappear alive 24 hours later in a location of your choice. You regain all your Stamina and Recoveries, and your choice of your Intuition or Presence increases by 1 (to a maximum of 6).</p>\n    <p>Additionally, choose a creature keyword belonging to one of the creatures who defeated you. Any creature with that keyword takes a bane on ability rolls against you.</p>\n    <p>If a hakaan gains this title as a result of using their Doomsight trait, they lose that trait.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "vgkLSPrJm1uqxnei", "name": "Reborn - Memories of the Beyond", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=vgkLSPrJm1uqxnei\n//@name=Reborn - Memories of the Beyond\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Reborn - Memories of the Beyond\",\n  description: `A god or other powerful being has determined that it is not yet your time to die. Your body fades away, but you reappear alive 24 hours later in a location of your choice. You regain all your Stamina and Recoveries, and your choice of your Intuition or Presence increases by 1 (to a maximum of 6).</p>\n    <p>Additionally, you have two skills of your choice.</p>\n    <p>If a hakaan gains this title as a result of using their Doomsight trait, they lose that trait.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "NVTmSznAt6KfY3GR", "name": "Theoretical Warrior", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=NVTmSznAt6KfY3GR\n//@name=Theoretical Warrior\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Theoretical Warrior\",\n  description: `Your choice of your Reason or Intuition increases by 1 (to a maximum of 6).</p>\n    <p>Additionally, choose a heroic ability belonging to any class. You gain this heroic ability, which can be paid for using the Heroic Resource of your class. You can’t use a heroic ability that requires a class feature you don’t have.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "cCxKGHWRyYZefnaj", "name": "Tireless - Bounce Back Fast", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=cCxKGHWRyYZefnaj\n//@name=Tireless - Bounce Back Fast\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Tireless - Bounce Back Fast\",\n  description: `Your choice of your Might or Agility increases by 1 (to a maximum of 6).</p>\n    <p>Additionally, whenever you rest for 8 hours or more, you can gain the benefit of a respite. Once you use this benefit, you can’t use it again until you have taken a regular respite.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "lc9hda6QtDMEyK8z", "name": "Tireless - Reserves of Strength", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=lc9hda6QtDMEyK8z\n//@name=Tireless - Reserves of Strength\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Tireless - Reserves of Strength\",\n  description: `Your choice of your Might or Agility increases by 1 (to a maximum of 6).</p>\n    <p>Additionally, your recovery value is half your Stamina.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "ztLDMXoh2G6TFlgs", "name": "Tireless - Undying", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=ztLDMXoh2G6TFlgs\n//@name=Tireless - Undying\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Tireless - Undying\",\n  description: `Your choice of your Might or Agility increases by 1 (to a maximum of 6).</p>\n    <p>Additionally, you can’t be affected by the bleeding condition.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "zD5I0SNyAUwaKk98", "name": "Unchained - Bye-Bye", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=zD5I0SNyAUwaKk98\n//@name=Unchained - Bye-Bye\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Unchained - Bye-Bye\",\n  description: `Your choice of your Might or Agility increases by 1 (to a maximum of 6).</p>\n    <p>Additionally, you can use a maneuver to teleport yourself and each willing ally within 5 squares of you to new positions within 10 squares of your original starting point. Once you use this benefit, you can’t use it again until you earn 1 or more Victories.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "QCdMvcyLDm793AIB", "name": "Unchained - Laughs at Locks", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=QCdMvcyLDm793AIB\n//@name=Unchained - Laughs at Locks\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Unchained - Laughs at Locks\",\n  description: `Your choice of your Might or Agility increases by 1 (to a maximum of 6).</p>\n    <p>Additionally, whenever you make a test to open a lock or break a restraint, you don’t need any tools and you automatically obtain a tier 3 outcome.</p>`,\n  source: \"Title\"\n});" }
{ "_id": "gLN9WBuv2aKfbPJI", "name": "Unchained - Slippery", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "icons/skills/trades/academics-merchant-scribe.webp", "command": "//@id=gLN9WBuv2aKfbPJI\n//@name=Unchained - Slippery\n//@img=icons/skills/trades/academics-merchant-scribe.webp\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Unchained - Slippery\",\n  description: `Your choice of your Might or Agility increases by 1 (to a maximum of 6).</p>\n    <p>Additionally, you can’t be grabbed against your will.</p>`,\n  source: \"Title\"\n});" }
