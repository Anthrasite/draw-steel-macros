{ "_id": "BZzgum3WNjxOqPt5", "name": "Can't Take Hold", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "modules/draw-steel-playtest-macros/icons/ancestry/human.png", "command": "//@id=BZzgum3WNjxOqPt5\n//@name=Can't Take Hold\n//@img=modules/draw-steel-playtest-macros/icons/ancestry/human.png\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Can't Take Hold\",\n  description: \"Your connection to the natural world allows you to resist certain supernatural effects. You ignore temporary difficult terrain created by magic and psionic abilities. Additionally, when you are force moved by a magic or psionic ability, you can reduce the forced movement distance by 1.\",\n  source: \"Ancestry\"\n});" }
{ "_id": "opxciTtfDub27wqC", "name": "Detect the Supernatural", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "modules/draw-steel-playtest-macros/icons/ancestry/human.png", "command": "//@id=opxciTtfDub27wqC\n//@name=Detect the Supernatural\n//@img=modules/draw-steel-playtest-macros/icons/ancestry/human.png\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Detect the Supernatural\",\n  description: \"As a maneuver, you open your awareness to detect supernatural creatures and phenomena. Until the end of your next turn, you know the location of any supernatural object, or any undead, construct, or creature from another world within 5 squares, even if you don’t have line of effect to that object or creature. You know if you’re detecting an item or a creature, and you know the nature of any creature you detect.\",\n  source: \"Ancestry\"\n});" }
{ "_id": "3CE6N1QyaXcGhnUS", "name": "Determination", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "modules/draw-steel-playtest-macros/icons/ancestry/human.png", "command": "//@id=3CE6N1QyaXcGhnUS\n//@name=Determination\n//@img=modules/draw-steel-playtest-macros/icons/ancestry/human.png\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Determination\",\n  description: \"A tolerance for pain and distress allows you to push through difficult situations. If you are frightened, slowed, or weakened, you can use a maneuver to immediately end one of those conditions.\",\n  source: \"Ancestry\"\n});" }
{ "_id": "sxAWSXDln8QkNLvj", "name": "Perseverance", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "modules/draw-steel-playtest-macros/icons/ancestry/human.png", "command": "//@id=sxAWSXDln8QkNLvj\n//@name=Perseverance\n//@img=modules/draw-steel-playtest-macros/icons/ancestry/human.png\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Perseverance\",\n  description: \"Giving up is for other people. You gain an edge on tests made using the Endurance skill. Additionally, when you are slowed, your speed is reduced to 3 instead of 2.\",\n  source: \"Ancestry\"\n});" }
{ "_id": "fX8m9UIvPdH0F5pJ", "name": "Resist the Unnatural", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "modules/draw-steel-playtest-macros/icons/ancestry/human.png", "command": "//@id=fX8m9UIvPdH0F5pJ\n//@name=Resist the Unnatural\n//@img=modules/draw-steel-playtest-macros/icons/ancestry/human.png\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Resist the Unnatural\",\n  description: \"Your instinctive resilience protects you from injuries beyond the routine. Whenever you take damage that isn’t untyped, you can use a triggered action to take half the damage.\",\n  source: \"Ancestry\"\n});" }
{ "_id": "WgVqK132pAhlfE7M", "name": "Staying Power", "scope": "global", "type": "script", "ownership": { "default": 0 }, "img": "modules/draw-steel-playtest-macros/icons/ancestry/human.png", "command": "//@id=WgVqK132pAhlfE7M\n//@name=Staying Power\n//@img=modules/draw-steel-playtest-macros/icons/ancestry/human.png\nawait game.macros.getName(\"ShareFeature\").execute({\n  activeActor: actor,\n  name: \"Staying Power\",\n  description: \"Your human physiology allows you to fight, run, and stay awake longer than others. You increase your number of Recoveries by 2.\",\n  source: \"Ancestry\"\n});" }
